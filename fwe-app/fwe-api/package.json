{
  "name": "fwe-api",
  "version": "1.0.0",
  "description": "FWE TypeScript Server for a time/task tracking app",
  "main": "./dist/src/server.js",
  "scripts": {
    "build": "tsc -b .",
    "start": "ts-node ./src/server.ts",
    "dev": "ts-node-dev ./src/server.ts",
    "lint": "tslint --project .",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "fixtures": "NODE_ENV=dev npx fixtures ./src/fixture --config ormconfig.js",
    "test": "jest --verbose --coverage --forceExit --detectOpenHandles --runInBand",
    "test:watch": "jest --watchAll --verbose --runInBand",
    "test:ci": "jest --verbose --coverage --ci --forceExit --detectOpenHandles --runInBand"
  },
  "keywords": [
    "FWE",
    "h-da",
    "API",
    "TypeScript",
    "Backend",
    "Tasks",
    "Tracking",
    "Time",
    "Labels"
  ],
  "author": "Ramon Walther <Ramon.Walther@stud.h-da.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.7",
    "@types/supertest": "^2.0.10",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-node-dev": "^1.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.7",
    "tslint-plugin-prettier": "^2.3.0",
    "typeorm-fixtures-cli": "^1.8.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.29"
  }
}
